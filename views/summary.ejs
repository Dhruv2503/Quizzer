<html>
    <head>
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css">
    </head>
    <body>
        <h1 style="text-align: center; ">
            <%= testsummary.testname%>
        </h1>
        <ul class="list-group">
            <li class="list-group-item">
                <b>
                    <i>
                        Name &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; Score
                    </i>
                </b>
            </li>
        <% for(let i=0;i<testsummary.participants.length;i++){%>
            <li class="list-group-item">
                <%= testsummary.participants[i].name %>
                &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
                <%= testsummary.participants[i].score %>
            </li>
        <%}%>
        </ul>
        <button type="button" class="btn btn-primary my-3 mx-3">
            <a href="/admin" style="color: black;">
                back
            </a>
        </button>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.min.js"></script>
    </body>
</html>